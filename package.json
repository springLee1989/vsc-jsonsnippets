{
  "name": "vsc-jsonsnippets",
  "displayName": "Snippet for JSON",
  "description": "Makes writing JSON fluent, with a simple set of snippets.",
  "icon": "images/json_logo.png",
  "publisher": "wmontalvo",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "0.6.0",
  "engines": {
    "vscode": "^1.9.1"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "json",
    "snippet",
    "create json",
    "javascript",
    "typescript",
    "emmet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wilsonmontalvo/vsc-jsonsnippets.git"
  },
  "contributes": {
    "snippets": [
      {
        "language": "json",
        "path": "./snippets/json.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/all-languages.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/all-languages.json"
      },
      {
        "language": "csharp",
        "path": "./snippets/all-languages.json"
      },
      {
        "language": "razor",
        "path": "./snippets/all-languages.json"
      }
    ]
  }
}
